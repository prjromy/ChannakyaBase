@model ChannakyaBase.Model.ViewModel.ChqRqstModel
@using ChannakyaBase.BLL
@using ChannakyaBase.BLL.Service
@{ 
    var dbd = @Model;
}
<div class="col-md-12">
    <div class="content-box">
        <fieldset>
            <legend style="font-size:large;font-weight:bold">Cheque Verification Details</legend>
            
            <table class="table-responsive">
                <tr>
                    <th>
                        Available Cheque Stock:-
                    </th>
                    <th colspan="2">@InformationUtilityService.AvailableChequeInStock()</th>
                </tr>
            </table>
            <br />
            <table class="table table-striped table-bordered table-responsive">
                <tbody>
                    
                    <tr>
                        <th>Account Number</th>

                        <th>Piecs</th>

                        <th>Transaction Date</th>
                        @*<th>PostedBy </th>
                            <td><span class="control-label">@Model.PostedUser</span></td>*@
                    </tr>
                    <tr>
                        <td><span class="control-label">@Model.AccountNo</span></td>
                        <td><span class="control-label">@Model.Pics</span></td>
                        @{
                            CommonService commonServices = new CommonService();
                        }
                        <td>  <span class="control-label">@commonServices.GetDate(Model.Tdate)</span></td>
                    </tr>


                </tbody>

            </table>
        </fieldset>
    </div>
</div>


