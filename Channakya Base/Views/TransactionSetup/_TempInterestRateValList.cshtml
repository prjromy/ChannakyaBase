@model IEnumerable<ChannakyaBase.DAL.DatabaseModel.TempIntRateVal>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Template Name")
        </th>
        <th>
            @Html.DisplayName("Interest Rate")
        </th>
        <th>
            @Html.DisplayName("Upper Limit Amount")
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TempIntRate.Tname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IRate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ULAmt)
        </td>
       
        <td>
            <button type="button" class="btn btn-info btn-sm btn-edit-TempIntRateVal" data-id="@item.TIRID" data-toggle="tooltip" title="Edit"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></button>
            <button type="button" class="btn btn-info btn-sm btn-sm btn-delete-TempIntRateVal" data-id="@item.TID" data-toggle="tooltip" title="Delete"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </td>
    </tr>
}

</table>
