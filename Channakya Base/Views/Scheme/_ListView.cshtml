@model ChannakyaAccounting.Models.ViewModel.SchemeDetailViewModel
@{
    var test = Model;
}




<tr class="detailsList">
    <td class="listDrill" id="@Model.SDID" style="vertical-align:top;font-weight:200">
       @Model.SDName
    </td>
    <td>
        @if (Model.SType == 0)
        {
            <span class="progress-description">
                Deposit
            </span>
        }
        else
        {
            <span class="progress-description">
                Loan
            </span>
        }
    </td>
    <td>
        @Model.SDID
    </td>
    <td>
      
    </td>

    <td>

        <td><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="btn btn-primary btn-xs editList" id="@Model.SDID" data-title="Edit"><span class="glyphicon glyphicon-pencil"></span></button></p></td>

        <td><p data-placement="top" data-toggle="tooltip" title="Delete"><button class="btn btn-danger btn-xs deleteNode" id="@Model.SDID" data-title="Delete"><span class="glyphicon glyphicon-trash"></span></button></p></td>

    </tr>
